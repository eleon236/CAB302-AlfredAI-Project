<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="TOP_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.cab302week4.controller.QuizResultsController"
>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button text="Continue" onAction="#onContinue"/>
    <Label text="Daily Quiz" alignment="CENTER"/>
    <Label text="Results" alignment="CENTER"/>

    <Label fx:id="resultsLabel"/>
    <VBox spacing="10.0" prefWidth="400" fx:id="questionsContainer"/>

</VBox>